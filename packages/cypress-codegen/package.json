{
  "name": "cypress-codegen",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "publishConfig": {
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  },
  "bin": {
    "cypress-codegen": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ExpediaGroup/cypress-codegen.git"
  },
  "bugs": {
    "url": "https://github.com/ExpediaGroup/cypress-codegen/issues"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/core": "7.22.10",
    "@babel/parser": "7.22.11",
    "@babel/types": "7.22.11",
    "@babel/generator": "7.22.10",
    "@types/babel__generator": "7.6.4",
    "chalk": "4.1.2",
    "commander": "11.0.0",
    "glob": "10.3.3",
    "prettier": "3.0.2"
  },
  "peerDependencies": {
    "cypress": ">=12"
  },
  "devDependencies": {
    "@swc/jest": "0.2.29",
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.4",
    "@types/node": "20.5.0",
    "jest": "29.6.4",
    "typescript": "5.1.6"
  },
  "scripts": {
    "build": "pnpm clean && tsc",
    "bump-version": "pnpm version $NEW_VERSION",
    "clean": "rm -rf dist",
    "lint": "eslint --quiet --fix src/**/*",
    "prepack": "pnpm build",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "jest": {
    "transform": {
      "^.+\\.(j|t)sx?$": "@swc/jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "clearMocks": true
  }
}
