{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "module": "esnext",
    "moduleResolution": "node",
    "lib": ["ESNext", "DOM"],
    "jsx": "react",
    "skipLibCheck": true,
    "types": ["bun", "cypress"],
    "noEmit": true,
    "noImplicitAny": true,
    "noUncheckedIndexedAccess": true,
    "strict": true,
    "target": "esnext"
  }
}
