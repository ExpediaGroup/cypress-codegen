{
  "name": "cypress-codegen",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@babel/parser": "7.18.5",
    "glob": "8.0.3"
  },
  "peerDependencies": {
    "cypress": ">=10"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@types/glob": "7.2.0",
    "@types/jest": "28.1.3",
    "cypress": "10.2.0",
    "eslint": "8.18.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-github": "4.3.6",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.5.3",
    "eslint-plugin-prettier": "4.1.0",
    "eslint-plugin-typescript": "0.14.0",
    "husky": "8.0.1",
    "jest": "28.1.1",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-scripts": "5.0.1",
    "semantic-release": "19.0.3",
    "ts-jest": "28.0.5",
    "typescript": "4.7.4",
    "webpack": "5.73.0"
  },
  "scripts": {
    "build": "tsc --skipLibCheck && mkdir -p node_modules/cypress-codegen/dist && cp -r dist package.json node_modules/cypress-codegen",
    "cypress:open": "cypress open",
    "cypress:component": "npm run build && cypress run --component",
    "cypress:e2e": "npm run build && cypress run",
    "format": "prettier --write .",
    "format-check": "prettier .",
    "lint": "eslint --quiet --fix src/**/*",
    "precypress:open": "npm run build",
    "prepare": "husky install",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.(j|t)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "clearMocks": true
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
