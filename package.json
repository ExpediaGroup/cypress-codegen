{
  "name": "cypress-codegen",
  "packageManager": "bun@1.2.2",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cypress-codegen": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ExpediaGroup/cypress-codegen.git"
  },
  "bugs": {
    "url": "https://github.com/ExpediaGroup/cypress-codegen/issues"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/core": "7.27.1",
    "@babel/generator": "7.27.1",
    "@babel/parser": "7.27.2",
    "@babel/types": "7.27.1",
    "chalk": "4.1.2",
    "commander": "14.0.0",
    "glob": "11.0.2",
    "prettier": "3.5.3"
  },
  "peerDependencies": {
    "cypress": ">=13"
  },
  "devDependencies": {
    "@swc/jest": "0.2.38",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/glob": "8.1.0",
    "@types/react": "19.1.5",
    "@types/jest": "29.5.14",
    "@types/node": "24.0.10",
    "cypress": "14.4.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "eslint": "9.27.0",
    "eslint-plugin-cypress": "4.3.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tsup": "8.5.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.32.1",
    "vite": "7.0.0"
  },
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --clean --dts --format cjs",
    "cypress:open": "cypress open",
    "cypress:component": "cypress run --component",
    "cypress:e2e": "cypress run",
    "format": "prettier --write .",
    "format-check": "prettier .",
    "lint": "eslint .",
    "prepack": "bun run build",
    "prepare": "husky",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "jest": {
    "transform": {
      "^.+\\.(j|t)sx?$": "@swc/jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "clearMocks": true
  }
}
